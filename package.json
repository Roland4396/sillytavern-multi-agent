{
  "name": "st-multi-agent",
  "version": "1.0.0",
  "description": "SillyTavern Multi-Agent Middleware - LangGraph based multi-agent system",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "node esbuild.config.mjs",
    "build:dev": "esbuild src/index.ts --bundle --format=esm --platform=browser --outfile=dist/index.js --external:@langchain/*",
    "dev": "node esbuild.config.mjs --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["sillytavern", "multi-agent", "langgraph", "langchain", "ai", "roleplay"],
  "author": "Roland",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^1.1.15",
    "@langchain/langgraph": "^1.1.0",
    "@langchain/openai": "^1.2.2"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "typescript": "^5.9.3"
  }
}
